<link rel="stylesheet" href="style.css">

<h2>List of Employees</h2>
<br>
<br>

<table>
    <tr><th>First Name</th><th>Last Name</th><th>Favorite donut</th></tr>
    <% for (var i = 0; i < emplist.length; i++) { %>
        <tr>
            <td><%= emplist[i].first_name %></td>
            <td><%= emplist[i].last_name %></td>
            <td><%= emplist[i].name %></td>
            <td><a href='/employees/<%=emplist[i].id%>/edit'>Edit</a></td>
        </tr>
   <% } %>
</table>